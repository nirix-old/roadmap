<% if flash[:success] %>
	<div class="alert alert-success">
		<button type="button" class="close" data-dismiss="alert">&times;</button>
		<%=flash[:success]%>
	</div>
<% end %>
<% if flash[:error] %>
	<div class="alert alert-error">
		<button type="button" class="close" data-dismiss="alert">&times;</button>
		<%=flash[:error]%>
	</div>
<% end %>
